package com.company;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s;
        System.out.print("Input your text: ");
        s=sc.nextLine();
        s=s.replaceAll(" {2,}", " ");
        s=s.trim();
        System.out.print(s);
    }
}
