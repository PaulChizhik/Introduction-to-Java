import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    String s;
    System.out.print("Исходная строка: ");
    s=sc.nextLine();
    int c=0;
    for(int i=0;i<s.length();i++){
      if(s.charAt(i)=='a'){
        c++;
      }
    }
    System.out.print("Результат: "+c);
  }
}
