package com.company;

import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("T(sec): ");
        int t=sc.nextInt();
        int h=t/3600;
        int m=t/60%60;
        int s=t%60;
        System.out.print(h+"h "+m+"min "+s+"sec");
    }
}
