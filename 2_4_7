package com.company;

public class Main {
    public static void main(String[] args) {
        f();
    }
    public static void f(){
        int factorial=1;
        int sum=0;
        for(int i=1;i<=9;i++){
            factorial*=i;
            if(i%2!=0) sum+=factorial;
        }
        System.out.print("Result: "+sum);
    }
}
